<?xml version="1.0" encoding="UTF-8"?><record_update sys_domain="global" table="sys_script_client">
    <sys_script_client action="DELETE">
        <active>true</active>
        <applies_extended>false</applies_extended>
        <condition/>
        <description>On a change of the Quantity Needed field, set the Total Cost to the new Quantity Needed value times the Item Cost value.&#13;
&#13;
NOTE: need to create another client script to execute this logic onChange of the Item Needed field.</description>
        <field>quantity_needed</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Set Requirement Total Cost</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
   if (isLoading) {
      return;
   }
	var formatMoney = function(number, decPlaces, decSep, thouSep) {
	decPlaces = isNaN(decPlaces = Math.abs(decPlaces)) ? 2 : decPlaces,
	decSep = typeof decSep === "undefined" ? "." : decSep;
	thouSep = typeof thouSep === "undefined" ? "," : thouSep;
	var sign = number < 0 ? "-" : "";
	var i = String(parseInt(number = Math.abs(Number(number) || 0).toFixed(decPlaces)));
	var j = (j = i.length) > 3 ? j % 3 : 0;

	return '$' + sign +
		(j ? i.substr(0, j) + thouSep : "") +
		i.substr(j).replace(/(\decSep{3})(?=\decSep)/g, "$1" + thouSep) +
		(decPlaces ? decSep + Math.abs(number - i).toFixed(decPlaces).slice(2) : "");
	};
	var qty = Number(newValue.split(',').join(''));
   //Set total cost to quantity needed * item cost
	var cost = g_form.getValue('item_cost');
	var totalCost = cost * qty;
	g_form.setValue('total_cost', totalCost, formatMoney(totalCost));
}]]></script>
